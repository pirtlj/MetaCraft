<h1>New map</h1>

<form id="new-map" name="map">
  <div class="field">
    <label for="game_id"> game_id:</label>
    <input type="text" name="game_id" id="game_id" value="<%= game_id %>" >
  </div>

  <div class="field">
    <label for="type"> type:</label>
    <input type="text" name="type" id="type" value="<%= type %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Map" />
  </div>

</form>

