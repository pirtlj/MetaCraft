<h1>Edit voxel</h1>

<form id="edit-voxel" name="voxel">
  <div class="field">
    <label for="map_id"> map_id:</label>
    <input type="text" name="map_id" id="map_id" value="<%= map_id %>" >
  </div>

  <div class="field">
    <label for="type"> type:</label>
    <input type="text" name="type" id="type" value="<%= type %>" >
  </div>

  <div class="field">
    <label for="x"> x:</label>
    <input type="text" name="x" id="x" value="<%= x %>" >
  </div>

  <div class="field">
    <label for="y"> y:</label>
    <input type="text" name="y" id="y" value="<%= y %>" >
  </div>

  <div class="field">
    <label for="z"> z:</label>
    <input type="text" name="z" id="z" value="<%= z %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Voxel" />
  </div>

</form>

