<h1>New tile</h1>

<form id="new-tile" name="tile">
  <div class="field">
    <label for="type"> type:</label>
    <input type="text" name="type" id="type" value="<%= type %>" >
  </div>

  <div class="field">
    <label for="x"> x:</label>
    <input type="text" name="x" id="x" value="<%= x %>" >
  </div>

  <div class="field">
    <label for="y"> y:</label>
    <input type="text" name="y" id="y" value="<%= y %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Tile" />
  </div>

</form>

